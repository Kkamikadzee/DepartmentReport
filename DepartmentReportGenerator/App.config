        <add key="DefaultFolder" value="reports" />
    </reportWinFormsSettings>

    <templateDocEditorSettings>
        <add key="ColumnTagExpression" value="\[=\$.+\$=\]" />
        <add key="FrontOffsetTag" value="3" />
        <add key="BackOffsetTag" value="6" />
    </templateDocEditorSettings>
    
